{
  "name": "fruity-shake",
  "version": "5.1.0",
  "description": "An Angular and Node Express application",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd ./public/app && npm install",
    "clear:all": "rm -rf node_modules && cd ./public/app && rm -rf node_modules",
    "ng:start": "cd ./public/app && tsc && npm start",
    "ng:build:prod": "cd ./public/app && tsc && npm run build:prod",
    "node:start": "tsc && nodemon --exec babel-node ./bin/www",
    "start": "concurrently \"npm run node:start\"  \"npm run ng:start\"",
    "app:build:run": "npm run ng:build:prod && npm run node:start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KShewengerz/ngx-express-typescript-starter.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "express",
    "babel",
    "express typescript",
    "angular express"
  ],
  "author": "Earl Hans Genoso <earlhansg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/earlhansg/fruity-shakes.git"
  },
  "homepage": "https://github.com/earlhansg/fruity-shakes.git#readme",
  "dependencies": {
    "@types/jasmine": "^2.8.16",
    "babel-plugin-module-resolver": "^3.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "serve-favicon": "~2.4.2",
    "ts-node": "~8.6.2"
  },
  "devDependencies": {
    "@angular/cli": "^9.0.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.5.5",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "~13.7.0",
    "@types/passport": "^0.4.5",
    "@types/passport-google-oauth": "^1.0.41",
    "concurrently": "^3.5.0",
    "nodemon": "^2.0.3",
    "tsc-watch": "^1.1.39",
    "tslint": "^6.0.0",
    "typescript": "^3.6.2",
    "typescript-register": "^1.1.0"
  }
}
